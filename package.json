{
  "name": "musolingo",
  "version": "0.0.1",
  "description": "Gameified music theory learning app",
  "main": "index.js",
  "engines": {
    "node": "6.9.x"
  },
  "scripts": {
    "start": "bin/setup && nodemon server/start.js",
    "dev": "check-node-version --node '>= 6.7.0' && bin/setup && nodemon server/start.js",
    "test-redux": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --compilers js:babel-register tests/redux/{*,**/*}.test.js",
    "test-routes": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --compilers js:babel-register tests/routes/{*,**/*}.test.js",
    "build": "check-node-version --node '>= 6.7.0' && bin/setup && webpack",
    "build-watch": "check-node-version --node '>= 6.7.0' && bin/setup && webpack -w --progress",
    "build-css": "node-sass -w public/keyboard.scss -o public/stylesheets",
    "seed": "node db/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpfiorilla/MusoLingo"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "sequelize",
    "postgresql",
    "e-commerce",
    "furniture"
  ],
  "bugs": {
    "url": "https://github.com/jpfiorilla/MusoLingo/issues"
  },
  "homepage": "https://github.com/jpfiorilla/MusoLingo#readme",
  "dependencies": {
    "aos": "^2.1.1",
    "axios": "^0.15.2",
    "babel-preset-es2015": "^6.18.0",
    "bcrypt-nodejs": "^0.0.3",
    "beautiful-piano": "0.0.6",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "chalk": "^1.1.3",
    "chance": "^1.0.4",
    "check-node-version": "^1.1.2",
    "cheerio": "^0.22.0",
    "dotenv": "^2.0.0",
    "epilogue": "^0.7.0",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "font-awesome": "^4.7.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.16.3",
    "nexusui": "^1.0.8",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "piano-game": "^1.2.0",
    "react": "^15.4.0",
    "react-addons-pure-render-mixin": "^15.4.0",
    "react-dom": "^15.3.2",
    "react-player": "^0.12.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "sendgrid": "^4.7.1",
    "sequelize": "^3.24.6",
    "stats.js": "^0.17.0",
    "tonal": "^0.65.0",
    "tone": "^0.8.0",
    "vexflow": "^1.2.83"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "chai-properties": "^1.2.1",
    "chai-things": "^0.2.0",
    "cookie-session": "^2.0.0-alpha.2",
    "dotenv": "^2.0.0",
    "enzyme": "^2.5.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.6",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.1",
    "volleyball": "^1.4.1",
    "webpack": "^1.13.3"
  }
}
